import java.util.Set;
import java.util.TreeSet;

public class SemanticAnalyzer
{    
   Set<String> SA = new TreeSet<String>();
   
   public boolean mainCheck (SemanticAction node)
   {
      for (int i = 0; i < node.getBranches().length; i++)
      {
         if (node.getBranches()[i] instanceof SemanticAction)
            mainCheck(node.getBranches()[i]);
         else
            if (node.getBranches()[i].toString() == "main" && 
                SA.contains(node.getBranches()[i].toString()) == false)
               return false;
            else 
               SA.add(node.getBranches()[i].toString());
      }
      return true;                 
   }
   
}
